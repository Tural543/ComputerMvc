@using ComputerStore.Class
@model List<Computer>
@{
    ViewData["Title"] = "GetComputers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <td>
                Name
            </td>
            <td>
                Marka
            </td>
            <td>
                Stock
            </td>
            <td>
                Salary
            </td>
        </tr>

    </thead>
    <tbody>
        @foreach(var com in Model)
        {
            <tr>
                <td>
                   @com.Name
                </td>
                <td>
                    @com.Marka.Name
                </td>
                <td>
                    @com.Stock
                </td>
                <td>
                    @com.Salary
                </td>
            </tr>
        }
    </tbody>
    

</table>

